      },
      "versionCode": 6,
      "package": "com.abebebe.onsen_maching",
      "name": "スーパー銭湯マッチング"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2d91b8d2-c544-467c-99a7-0830e9569fdc"
      }
